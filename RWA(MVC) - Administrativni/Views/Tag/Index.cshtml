@model List<RWA_MVC____2.Models.Tag>
@{
}
<h1>Tags</h1>
<table class="table">
   <thead>
       <tr>
           <th>Id</th>
           <th>Name</th>
       </tr>
   </thead>
   <tbody>
       @foreach (var tag in Model)
        {
            <tr>
                <td>@tag.Id</td>
                <td>@tag.Name</td>
                <td><a href="Tag/View/@tag.Id" class="btn btn-dark">View</a></td>
            </tr>
        }
   </tbody>
</table>